{
    "name": "mtinypng",
    "version": "0.0.1",
    "description": "基于TinyPng的图片压缩工具",
    "main": "index.js",
    "scripts": {
        "build": "cd app && npm run build && cd ..",
        "start": "npm run build && electron ./app .",
        "pack": "npm run build && electron-packager ./app 'MTinyPng' --platform=mas --overwrite --out='./dist' --icon='./app/src/assets/images/mtiny.icns'"
