    "bugs": {
        "url": "https://github.com/web677/MTinyPng/issues"
    },
    "homepage": "https://github.com/web677/MTinyPng#readme",
    "devDependencies": {
        "babel-preset-es2015": "^6.24.1",
        "electron": "^2.0.2",
        "electron-packager": "^12.1.0",
        "tinify": "^1.5.0"
    },
    "dependencies": {}
}
